
@import "cd";

.page-wrapper {
    background: url(<%= asset_path 'noise.png' %>) repeat 0 0, 
        linear-gradient(to bottom, #333 0, #222 70%, #1B1D20 90%, #0B0B0B 100%);
}

.order-detail-cont {
	background: url(<%= asset_path 'beige-paper.png' %>) 0% 0% / 196px 196px repeat, 
            linear-gradient($darken-10 .1em, transparent .1em) 0% 0% / 100% 2em repeat,
            linear-gradient($color1 0, desaturate(darken($color1, 10%), 10%) 100%) no-repeat;
}